{
	"name": "indicalabs/yii2-plupload",
	"description": "Yii2 Plupload extension, provides useful widgets.",
	"type": "yii2-extension",
	"keywords": ["yii2", "plupload", "extension", "widget"],
	"homepage": "https://github.com/indicalabs/yii2-plupload",
	"license": "MIT",
	"authors": [
        {
            "name": "Venu Narukulla",
            "email": "venu.narukulla@gmail.com"
        }
    ],
	"require": {
		"yiisoft/yii2": ">=2.0.13.1",
	    "npm-asset/plupload": "2.*"
	},
    "require-dev": {
    	"phpunit/phpunit": "7.0.0"
 	 },
 	 
    "autoload": {
        "psr-4": { 
        	"indicalabs\\plupload\\": "" 
        }
    },
    "repositories" : [{
			"type" : "composer",
			"url" : "https://asset-packagist.org"
		}
	],
	"extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
